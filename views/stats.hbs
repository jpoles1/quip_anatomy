<a class="btn btn-primary" href="export" target="_blank">Export Game Data</a>
<br><br>
<table border="1" class="dataframe">
  <thead>
    <tr style="text-align: center;">
      <th>Lesson</th>
      <th>Term</th>
      <th>Score</th>
      <th>Answer Count</th>
      <th>Cumulative Score</th>
    </tr>
  </thead>
  <tbody style="text-align: center;">
    {{#each gamestats}}
      {{#if this}}
        <tr>
          <td><b>{{this.lesson}}</b></td>
          <td><b>{{this.term}}</b></td>
          <td>{{this.score}}</td>
          <td>{{this.ct}}</td>
          <td>{{this.cumscore}}</td>
        </tr>
      {{/if}}
    {{/each}}
  </tbody>
</table>
<br>
